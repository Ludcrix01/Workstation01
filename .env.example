# MJWorkHub example environment variables
# Copy this file to .env and fill values in production or local development

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/mjworkhub

# Secret used to encrypt provider secrets (must be 32 bytes in production)
MJ_SECRET_KEY=change_me_to_a_strong_secret_32_chars

# Test server / frontend proxy
API_TARGET=http://localhost:4000

# Ports
TEST_SERVER_PORT=4000
FRONTEND_PORT=3000

# Node env
NODE_ENV=development

# Base URL used to build redirect URIs for OAuth (ex: http://localhost:3000 or the public URL)
APP_BASE_URL=http://localhost:3000

